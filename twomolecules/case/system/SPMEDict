FoamFile
{
    version         2.0;
    format          ascii;

    root            "";
    case            "";
    instance        "";
    local           "";

    class           dictionary;
    object          SPMEDict;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

alpha		3.0e9;//contribution of real/reciprocal space
K		(64 64 64);//No. of grid points (reciprocalspace wave vectors)
n		4;//B-Spline interpolation order
activate	no;	

